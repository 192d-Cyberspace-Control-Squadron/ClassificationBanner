Windows Registry Editor Version 5.00

; Classification Banner - SECRET Configuration
; Import this file to configure the banner for SECRET

[HKEY_CURRENT_USER\SOFTWARE\ClassificationBanner]
"Classification"="SECRET"
"Enabled"=dword:00000001
"FPCON"="Alpha"
"CPCON"="1"

; System Information Display Flags (1 = show, 0 = hide)
"ShowHostname"=dword:00000001
"ShowUsername"=dword:00000001
"ShowIPAddress"=dword:00000001